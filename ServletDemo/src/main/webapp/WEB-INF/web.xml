<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DemoServlet</servlet-name>
  	<display-name>DemoServlet</display-name>
  	<description></description>
  	<servlet-class>DemoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NameServlet</servlet-name>
  	<display-name>NameServlet</display-name>
  	<description></description>
  	<servlet-class>NameServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NumberServlet</servlet-name>
  	<display-name>NumberServlet</display-name>
  	<description></description>
  	<servlet-class>NumberServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DemoServlet</servlet-name>
  	<url-pattern>/DemoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NameServlet</servlet-name>
  	<url-pattern>/NameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NumberServlet</servlet-name>
  	<url-pattern>/NumberServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
